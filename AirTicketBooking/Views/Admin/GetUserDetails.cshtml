@model List<AirTicketBooking.Models.UserReg>

@{
    ViewBag.Title = "GetUserDetails";
    Layout = "~/Views/Shared/AdminLayout.cshtml";
}



<p>
    @Html.ActionLink("Create New", "Create")
</p>
<table class="table">
    <thead>
        <tr>
            <!-- Add table headers for user properties here -->
            <th>First Name</th>
            <th>Last Name</th>
            <th>Date of Birth</th>
            <th>Age</th>
            <th>Email</th>
            <th>Phone Number</th>
            <th>Action</th>
        </tr>
    </thead>
    <tbody>
        @foreach (var user in Model)
        {
            <tr>
                <!-- Add table cells to display user properties here -->
                <td>@user.firstName</td>
                <td>@user.lastName</td>
                <td>@user.DateofBirth.ToShortDateString()</td>
                <td>@user.Age</td>
                <td>@user.Email</td>
                <td>@user.phoneNumber</td>
                <td>
                    @Html.ActionLink("Edit", "EditUserDetailsById", new { id = user.userId }) |
                    @Html.ActionLink("Details", "GetUserDetailsById", new { id = user.userId }) |
                    @Html.ActionLink("Delete", "DeleteUserDetailById", new { id = user.userId })
                </td>
            </tr>
        }
    </tbody>
</table>
@if (TempData["result1"] != null)
{
    <script type="text/javascript">
        alert("Record Is Inserted Successfully");
    </script>

}
@if (TempData["result2"] != null)
{
    <script type="text/javascript">
        alert("Record Is Updated Successfully");
    </script>

}
@if (TempData["result3"] != null)
{
    <script type="text/javascript">
        alert("Record Is Deleted Successfully");
    </script>

}   